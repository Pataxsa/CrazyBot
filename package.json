{
	"name": "crazybot",
	"version": "1.3.0",
	"description": "A multipurpose discord bot to help students.",
	"main": "src/bot.ts",
	"type": "module",
	"private": "true",
	"contributors": ["0BL1V10N (https://github.com/0BL1V10N1)"],
	"scripts": {
		"start": "tsx .",
		"dev": "tsx --watch .",
		"check": "biome check",
		"check:fix": "biome check --fix",
		"format": "biome format",
		"format:fix": "biome format --fix",
		"lint": "biome lint",
		"lint:fix": "biome lint --fix",
		"type-check": "tsc --noEmit",
		"prepare": "node .husky/install.mjs"
	},
	"homepage": "https://github.com/0BL1V10N1/CrazyBot#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/0BL1V10N1/CrazyBot.git"
	},
	"keywords": ["discord-bot", "typescript", "docker"],
	"license": "MIT",
	"dependencies": {
		"discord.js": "^14.17.3",
		"tsx": "^4.19.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.1",
		"husky": "^9.1.7",
		"typescript": "^5.7.3"
	},
	"engines": {
		"node": ">=20",
		"npm": ">=10",
		"pnpm": ">=9"
	}
}
