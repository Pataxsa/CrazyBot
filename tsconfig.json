{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["*"]
		}
	},
	"exclude": ["node_modules", "dist"]
}
